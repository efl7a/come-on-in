<%= render :partial => 'study_session', :locals => {session: @study_session} %>
<% if @study_session.students.count > 0 %>
  <% @study_session.students.each do |student| %>
  <%= student.name %> | <%= student.current_grade %>
  <% end %>
<% else %>
  <p>No students have signed up for this study session.</>
<% end %>
<br><br>
