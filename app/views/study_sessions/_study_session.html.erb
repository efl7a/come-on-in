<%= session.subject %> | <%= link_to session.content, study_session_path(session) %>
<br><br>
<%= session.date.strftime("%A, %b %d, %Y") %> at <%= session.time.strftime("%H:%M") %>
<br><br>
<% if current_user.teacher? || current_user.admin? %>
  <%= render :partial => "study_sessions/teacher_navigation", :locals => {session: session} %>
<% else %>
  <%= render :partial => "study_sessions/student_navigation", :locals => {session: session} %>
<% end %>
